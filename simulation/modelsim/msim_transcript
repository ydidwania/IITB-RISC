vsim work.control_path(behave)
# vsim work.control_path(behave) 
# Start time: 15:57:23 on Oct 24,2018
# Loading std.standard
# Loading std.textio(body)
# Loading ieee.std_logic_1164(body)
# Loading work.control_path(behave)
# Loading work.mux_state(behave)
add wave -position insertpoint  \
sim:/control_path/opcode
add wave -position insertpoint  \
sim:/control_path/s1
add wave -position insertpoint  \
sim:/control_path/current_state
add wave -position insertpoint  \
sim:/control_path/next_state
force -freeze sim:/control_path/opcode 0101000000000000 0
run
